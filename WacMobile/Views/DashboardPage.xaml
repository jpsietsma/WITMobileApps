<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WacMobile.Views.DashboardPage">
    <ContentPage.Content>
        <StackLayout Orientation="Horizontal">
            <Label Text="Currently Logged in as: " VerticalOptions="Center" HorizontalOptions="Center" />
            <Label Text="{Binding suFullName}" VerticalOptions="Center" HorizontalOptions="Center" />
        </StackLayout>        
    </ContentPage.Content>
</ContentPage>